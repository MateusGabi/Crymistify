{
  "name": "remind-web-app",
  "version": "1.4.7",
  "private": true,
  "dependencies": {
    "commitizen": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "express": "^4.16.2",
    "feather-icons": "^4.5.0",
    "firebase": "^5.1.0",
    "generate-release": "^0.14.0",
    "gestalt": "^0.82.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "logrocket": "^0.6.10",
    "material-design-lite": "^1.3.0",
    "moment": "^2.19.2",
    "react": "^16.8.6",
    "react-dom": "^16.0.0",
    "react-scripts": "2.0.3",
    "rxjs": "^5.5.2",
    "semantic-release": "^15.1.4"
  },
  "scripts": {
    "build": "react-scripts build",
    "cm": "npm run prettier && git-cz",
    "deploy": "git push heroku master",
    "eject": "react-scripts eject",
    "gs": "generate-release",
    "heroku-postbuild": "npm run build",
    "lint": "./node_modules/.bin/eslint src/**/*.js",
    "prettier": "prettier --single-quote --trailing-comma es5 --write '{src,__{tests,mocks}__}/**/*.js'",
    "release": "gpom & npm run deploy",
    "semantic-release": "semantic-release --dry-run=true --no-ci=true",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "react-scripts test --env=jsdom",
    "test:coverage:ci": "npm test -- --coverage"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@storybook/react": "^4.0.0",
    "prettier": "1.15.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
