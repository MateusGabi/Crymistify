{
  "name": "remind-web-app",
  "version": "1.1.0-0",
  "private": true,
  "dependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "express": "^4.16.2",
    "feather-icons": "^4.5.0",
    "firebase": "^4.5.2",
    "generate-release": "^0.14.0",
    "gestalt": "^0.62.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "logrocket": "^0.5.6",
    "material-design-lite": "^1.3.0",
    "moment": "^2.19.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts": "1.0.14",
    "rxjs": "^5.5.2",
    "semantic-release": "^15.1.4"
  },
  "scripts": {
    "cm": "npm run prettier && git-cz",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage:ci": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "heroku-postbuild": "npm run build",
    "deploy": "git push heroku master",
    "semantic-release": "semantic-release --dry-run=true --no-ci=true",
    "prettier": "prettier --single-quote --trailing-comma es5 --write '{src,__{tests,mocks}__}/**/*.js'",
    "release": "gpom & npm run deploy",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "gs": "generate-release"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@storybook/react": "^3.3.15",
    "prettier": "1.11.1"
  }
}
